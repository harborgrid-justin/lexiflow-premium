{
  "name": "lexiflow-backend",
  "version": "1.0.0",
  "description": "LexiFlow Enterprise Backend - Document Management System",
  "main": "dist/src/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc\" nest start --watch",
    "start:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --expose-gc --inspect\" nest start --watch",
    "start:prod": "node --max-old-space-size=2048 --max-semi-space-size=16 --expose-gc --optimize-for-size dist/src/main",
    "start:prod:memory": "node --max-old-space-size=2048 --max-semi-space-size=16 --expose-gc --optimize-for-size --gc-interval=100 --max-old-generation-size-mb=1536 dist/src/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathPattern=\\.spec\\.ts$",
    "test:integration": "jest --testPathPattern=\\.integration\\.ts$",
    "typecheck": "tsc --noEmit",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/config/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/config/seeds/seed.ts",
    "seed:test": "ts-node -r tsconfig-paths/register src/config/seeds/seed.ts --env=test",
    "db:clean": "ts-node -r tsconfig-paths/register scripts/clean-db.ts",
    "db:sync": "ts-node -r tsconfig-paths/register scripts/sync-schema.ts",
    "db:schema": "npm run db:clean && npm run db:sync",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed",
    "db:fresh": "npm run db:clean && npm run seed",
    "db:test": "ts-node -r tsconfig-paths/register src/scripts/test-db-connection.ts",
    "db:drop-all": "node scripts/drop-all-tables.js",
    "db:create-drafting": "ts-node -r tsconfig-paths/register src/database/scripts/create-drafting-tables.ts",
    "seed:drafting": "ts-node -r tsconfig-paths/register src/database/scripts/seed-drafting.ts",
    "db:init-snake-case": "ts-node -r tsconfig-paths/register scripts/init-db-snake-case.ts",
    "db:reset-snake-case": "node scripts/reset-db-snake-case.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "validate": "npm run typecheck && npm run lint:check && npm run test",
    "validate:full": "npm run typecheck && npm run lint:check && npm run test:cov && npm run test:e2e",
    "memory:snapshot": "node --expose-gc --heap-prof dist/src/main",
    "memory:analysis": "node --trace-gc --trace-gc-verbose dist/src/main"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@faker-js/faker": "^10.1.0",
    "@google/generative-ai": "^0.24.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@nestjs/apollo": "^13.2.3",
    "@nestjs/bull": "11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/platform-socket.io": "^11.1.10",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.10",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "dataloader": "^2.2.3",
    "googleapis": "^169.0.0",
    "graphql": "^16.12.0",
    "graphql-subscriptions": "^3.0.0",
    "helmet": "^8.1.0",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^18.0.2",
    "mime-types": "^3.0.2",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react-hot-toast": "^2.6.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^7.0.0",
    "typeorm": "^0.3.28",
    "typeorm-naming-strategies": "^4.1.0",
    "ua-parser-js": "^1.0.38",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.2",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
