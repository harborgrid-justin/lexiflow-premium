{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1",
    "^@shared/(.*)$": "<rootDir>/../src/shared/$1",
    "^@shared$": "<rootDir>/../src/shared",
    "^@core/(.*)$": "<rootDir>/../src/$1",
    "^@core$": "<rootDir>/../src",
    "^@auth/(.*)$": "<rootDir>/../src/auth/$1",
    "^@auth$": "<rootDir>/../src/auth",
    "^@security/(.*)$": "<rootDir>/../src/security/$1",
    "^@security$": "<rootDir>/../src/security",
    "^@common/(.*)$": "<rootDir>/../src/common/$1",
    "^@common$": "<rootDir>/../src/common",
    "^@config/(.*)$": "<rootDir>/../src/config/$1",
    "^@config$": "<rootDir>/../src/config",
    "^@database/(.*)$": "<rootDir>/../src/database/$1",
    "^@database$": "<rootDir>/../src/database",
    "^@errors/(.*)$": "<rootDir>/../src/errors/$1",
    "^@errors$": "<rootDir>/../src/errors",
    "^@monitoring/(.*)$": "<rootDir>/../src/monitoring/$1",
    "^@monitoring$": "<rootDir>/../src/monitoring",
    "^@performance/(.*)$": "<rootDir>/../src/performance/$1",
    "^@performance$": "<rootDir>/../src/performance",
    "^@users/(.*)$": "<rootDir>/../src/users/$1",
    "^@users$": "<rootDir>/../src/users",
    "^@cases/(.*)$": "<rootDir>/../src/cases/$1",
    "^@cases$": "<rootDir>/../src/cases",
    "^@documents/(.*)$": "<rootDir>/../src/documents/$1",
    "^@documents$": "<rootDir>/../src/documents",
    "^@discovery/(.*)$": "<rootDir>/../src/discovery/$1",
    "^@discovery$": "<rootDir>/../src/discovery",
    "^@evidence/(.*)$": "<rootDir>/../src/evidence/$1",
    "^@evidence$": "<rootDir>/../src/evidence",
    "^@billing/(.*)$": "<rootDir>/../src/billing/$1",
    "^@billing$": "<rootDir>/../src/billing",
    "^@analytics/(.*)$": "<rootDir>/../src/analytics/$1",
    "^@analytics$": "<rootDir>/../src/analytics",
    "^@communications/(.*)$": "<rootDir>/../src/communications/$1",
    "^@communications$": "<rootDir>/../src/communications",
    "^@integrations/(.*)$": "<rootDir>/../src/integrations/$1",
    "^@integrations$": "<rootDir>/../src/integrations",
    "^@graphql/(.*)$": "<rootDir>/../src/graphql/$1",
    "^@graphql$": "<rootDir>/../src/graphql",
    "^@realtime/(.*)$": "<rootDir>/../src/realtime/$1",
    "^@realtime$": "<rootDir>/../src/realtime",
    "^@types/(.*)$": "<rootDir>/../src/types/$1",
    "^@types$": "<rootDir>/../src/types",
    "^@sharedTypes/(.*)$": "<rootDir>/../src/shared-types/$1",
    "^@sharedTypes$": "<rootDir>/../src/shared-types",
    "^@case-teams/(.*)$": "<rootDir>/../src/case-teams/$1",
    "^@case-teams$": "<rootDir>/../src/case-teams",
    "^@tasks/(.*)$": "<rootDir>/../src/tasks/$1",
    "^@tasks$": "<rootDir>/../src/tasks",
    "^@organizations/(.*)$": "<rootDir>/../src/organizations/$1",
    "^@organizations$": "<rootDir>/../src/organizations"
  },
  "setupFilesAfterEnv": ["<rootDir>/setup.ts"],
  "testTimeout": 30000
}
