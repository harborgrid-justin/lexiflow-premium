# LexiFlow Frontend Environment Variables

# Backend API Configuration (NestJS backend using Neon PostgreSQL)
VITE_API_URL=http://localhost:5000

# WebSocket URL (when enabled)
VITE_WS_URL=ws://localhost:5000

# Google Gemini API Key (for Legal Research AI)
GEMINI_API_KEY=your_api_key_here

# Feature Flags
VITE_USE_BACKEND_API=true

# Environment
VITE_ENV=development
